local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("KFX|Hub V2.0", "DarkTheme")

--Credits
local Tab = Window:NewTab("Credits")
local Section = Tab:NewSection("UI By XHeptc")
local Section = Tab:NewSection("Hub By Nope#3473")

--Player
local Tab = Window:NewTab("Player")
local Section = Tab:NewSection("Character")
Section:NewSlider("Player Speed", "Your sonic right?", 300, 16, function(s) -- 500 (MaxValue) | 0 (MinValue)
	game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = s
end)
Section:NewSlider("Player JP", "Be like that Moon Cow", 250, 50, function(s) -- 500 (MaxValue) | 0 (MinValue)
	game.Players.LocalPlayer.Character.Humanoid.JumpPower = s
end)
Section:NewButton("Player Fly (G)", "Fly if you press g", function()
	loadstring(game:HttpGet("https://raw.githubusercontent.com/GiveUsername/KFX-Hub-2.0/main/Toggle%20fly%20script"))()
end)

--Games
local Tab = Window:NewTab("Games")
